{
    "name": "jalno/graphql",
    "version": "0.0.1",
    "description": "Jalno GraphQL",
    "autoload": {
        "psr-4": {
            "Jalno\\GraphQL\\": "src/"
        }
    },
    "require": {
        "jalno/lumen": "^0.0.1",
        "webonyx/graphql-php": "^14.8.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.7.6",
        "orchestra/testbench": "^6.18.0"
    },
    "scripts": {
        "test:types": "./vendor/phpstan/phpstan/phpstan analyse -vvv --debug --memory-limit 256M phpstan.neon src",
        "test": [
            "@test:types"
        ]
    },
    "extra": {
        "jalno": {
            "package": "Jalno\\GraphQL\\Package"
        }
    }
}
